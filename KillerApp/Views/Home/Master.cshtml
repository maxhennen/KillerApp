@Page MasterPageFile = "/Home/Master" 
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>MobielKopen.nl</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Style/bootstrap.css" rel="stylesheet" />
    <link href="~/Style/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Style/shop-homepage.css" rel="stylesheet" />
    <script src="~/scripts/JavaScript.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Navigation -->
    <div class="navbar-header">
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">MobielKopen.nl</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
           @if (this.Session["Header"] == null)
           {
             <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="#">Producten</a>
                    </li>
                    <li>
                        <a href="#">Contact</a>
                    </li>
                </ul>

                 <form method="post" id="login" class="navbar-form navbar-right" role="form" action="/Home/Login">
                     <div class="input-group">
                         <input id="email" type="email" class="form-control" name="EmailText" value="" placeholder="Email Address">
                     </div>
                     <div class="input-group">
                         <input id="password" type="password" class="form-control" name="WachtwoordText" value="" placeholder="Password">
                     </div>
                     <button type="submit" class="btn btn-primary" >Login</button>
                 </form>
                 <form method="post" id="registreren" class="navbar-form navbar-right" role="form"  action="/Home/RegistrerenButton">
                     <button type="submit" class="btn btn-primary">Registreren</button>
                 </form>
            </div>
                }
               else if ((int)this.Session["Header"] == 1)
               {
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="#">Producten</a>
                        </li>
                        <li>
                            <a href="#">Mijn account</a>
                        </li>
                        <li>
                            <a href="#">Contact</a>
                        </li>
                    </ul>

                    <form method="post" id="signin" class="navbar-form navbar-right" role="form" action="/Home/Loguit">
                        <button type="submit" class="btn btn-primary">Uitloggen</button>
                    </form>
                </div>
               }

               else if ((int)this.Session["Header"] == 2)
               {
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="#">Producten</a>
                        </li>
                        <li>
                            <a href="#">Mijn account</a>
                        </li>
                        <li>
                            <a href="#">Contact</a>
                        </li>
                    </ul>

                    <form method="post" id="signin" class="navbar-form navbar-right" role="form" action="/Home/Loguit">
                        <button type="submit" class="btn btn-primary">Uitloggen</button>
                    </form>
                </div>
           }

            else if ((int)this.Session["Header"] == 3)
            {
               <script type="text/javascript">
                   GegevensNietBekend();
               </script>
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="#">Producten   </a>
                        </li>
                        <li>
                            <a href="#">Mijn account</a>
                        </li>
                        <li>
                            <a href="#">Contact</a>
                        </li>
                    </ul>

                    <form method="post" id="signin" class="navbar-form navbar-right" role="form" action="/Home/Login">
                        <div class="input-group">
                            <input id="email" type="email" class="form-control" name="EmailText" value="" placeholder="Email Address">
                        </div>
                        <div class="input-group">
                            <input id="password" type="password" class="form-control" name="WachtwoordText" value="" placeholder="Password">
                        </div>
                        <button type="submit" class="btn btn-primary">Login</button>
                    </form>
                </div>
            }

            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    </div>
    <!-- Page Content -->
    <td><asp:contentplaceholder id="Content" runat="server" /></td>
    <!-- /.container -->
    <div class="container">
        <hr>
        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Max Hennen 2017</p>
                </div>
            </div>
        </footer>
    </div>
    <!-- /.container -->
    <!-- jQuery -->
    <script src="~/scripts/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/bootstrap.js"></script>
</body>
</html>
 
